mutate(aux = as.character(monthyear),
year = substr(aux,1,4),
month = substr(aux,5,6),
date = as.Date(paste(year, month, '01'), format = '%Y %m %d')) %>%
group_by(actor1countrycode) %>%
filter(sum(numevents) > 200000) %>% # Mexico tenía como 700,000
